{
  "name": "ai-project-prompter",
  "version": "0.1.2",
  "description": "A React component for exploring project files and crafting AI prompts with their contents",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ai-project-prompter": "./dist/bin/ai-project-prompter.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prompter": "node ./dist/bin/ai-project-prompter.js"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "reactflow": "^11.10.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "express": "^4.18.2",
    "globals": "^15.9.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  },
  "keywords": [
    "ai",
    "prompt",
    "file-explorer",
    "react",
    "typescript",
    "project-files"
  ],
  "author": "Federico Del Gaudio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/delaudio/ai-project-prompter"
  },
  "bugs": {
    "url": "https://github.com/delaudio/ai-project-prompter/issues"
  },
  "homepage": "https://github.com/delaudio/ai-project-prompter#readme"
}
